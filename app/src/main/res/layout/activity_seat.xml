        <ImageView
            android:id="@+id/view_table23"
            android:layout_width="35dp"
            android:layout_height="50dp"
            android:layout_below="@+id/view_table26"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="20dp"
            android:background="#767677" />

        <ImageView
            android:id="@+id/view_table22"
            android:layout_width="35dp"
            android:layout_height="50dp"
            android:layout_below="@+id/view_table25"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:layout_toRightOf="@+id/view_table23"
            android:background="#767677" />

        <ImageView
            android:id="@+id/view_table21"
            android:layout_width="35dp"
            android:layout_height="50dp"
            android:layout_below="@+id/view_table24"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:layout_toRightOf="@+id/view_table22"
            android:background="#767677" />

        <ImageView
            android:id="@+id/view_table20"
            android:layout_width="50dp"
            android:layout_height="35dp"
            android:layout_below="@+id/view_div1"
            android:layout_marginLeft="10dp"
