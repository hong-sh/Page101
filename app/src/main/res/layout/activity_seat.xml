<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_pageimg"
    tools:context="com.application.page101.PointActivity">

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/layout_bottom"
        android:layout_margin="16dp"
        android:background="@drawable/round_box_frame"
        android:padding="10dp">

        <ImageView
            android:id="@+id/imageView_renew"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:clickable="true"
            android:src="@drawable/icon_seat_renew" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/imageView_renew"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <View
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:background="#767677" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:text="빈 좌석"
                    android:textColor="#515151"
                    android:textSize="11dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <View
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:background="#E29700" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:text="사용 좌석"
                    android:textColor="#515151"
                    android:textSize="11dp" />
            </LinearLayout>
        </LinearLayout>

        <View
            android:id="@+id/view_div1"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentTop="true"
            android:layout_marginRight="110dp"
            android:layout_marginTop="115dp"
            android:background="#515151" />


        <View
            android:id="@+id/view_div2"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="90dp"
            android:background="@android:color/transparent" />

        <View
            android:id="@+id/view_div3"
            android:layout_width="1dp"
            android:layout_height="170dp"
            android:layout_below="@+id/view_div1"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@+id/view_table19"
            android:background="#515151" />

        <View
            android:id="@+id/view_div4"
            android:layout_width="1dp"
            android:layout_height="60dp"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@+id/view_table8"
            android:background="#515151" />

        <View
            android:id="@+id/view_div5"
            android:layout_width="1dp"
            android:layout_height="60dp"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@+id/view_table4"
            android:background="#515151" />


        <View
            android:id="@+id/view_table26"
            android:layout_width="50dp"
            android:layout_height="35dp"
