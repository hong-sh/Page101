            android:layout_toRightOf="@+id/view_div4"
            android:background="#767677" />

        <ImageView
            android:id="@+id/view_table6"
            android:layout_width="50dp"
            android:layout_height="35dp"
            android:layout_above="@+id/view_table4"
            android:layout_marginBottom="15dp"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/view_table7"
            android:background="#767677" />

        <ImageView
            android:id="@+id/view_table2"
            android:layout_width="50dp"
            android:layout_height="35dp"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@+id/view_div5"
            android:background="#767677" />


    </RelativeLayout>


    <LinearLayout
        android:id="@+id/layout_bottom"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:layout_alignParentBottom="true"
        android:background="#e2e2e2"
        android:gravity="center"
        android:orientation="horizontal"
        android:padding="5dp">

        <ImageView
            android:id="@+id/button_home"
            android:layout_width="35dp"
            android:layout_height="45dp"
            android:clickable="true"
            android:scaleType="fitXY"
            android:src="@drawable/icon_menu_home" />

        <ImageView
            android:id="@+id/button_reservation"
            android:layout_width="35dp"
            android:layout_height="45dp"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="15dp"
            android:clickable="true"
            android:scaleType="fitXY"
            android:src="@drawable/icon_menu_reservation" />

        <ImageView
            android:id="@+id/button_seat"
            android:layout_width="35dp"
            android:layout_height="45dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="30dp"
            android:clickable="true"
            android:scaleType="fitXY"
            android:src="@drawable/icon_menu_c_seat" />

        <ImageView
            android:id="@+id/button_event"
            android:layout_width="35dp"
            android:layout_height="45dp"
            android:clickable="true"
            android:scaleType="fitXY"
            android:src="@drawable/icon_menu_event" />
    </LinearLayout>

    <ProgressBar
        android:id="@+id/progressBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:visibility="gone" />


</RelativeLayout>
