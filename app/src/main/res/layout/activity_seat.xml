            android:layout_toRightOf="@id/view_div3"
            android:background="#767677" />

        <TextView
            android:id="@+id/view_table_counter"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_alignParentRight="true"
            android:layout_below="@id/view_div1"
            android:layout_marginTop="10dp"
            android:background="@drawable/button_white"
            android:gravity="center"
            android:text="카운터"
            android:textColor="#515151"
            android:textSize="12dp" />

        <ImageView
            android:id="@+id/view_table3"
            android:layout_width="50dp"
            android:layout_height="35dp"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/view_table_counter"
            android:layout_marginRight="50dp"
            android:layout_marginTop="10dp"
            android:background="#767677" />

        <ImageView
            android:id="@+id/view_table1"
            android:layout_width="50dp"
            android:layout_height="35dp"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/view_table3"
            android:layout_marginTop="20dp"
            android:background="#767677" />

        <ImageView
            android:id="@+id/view_table9"
            android:layout_width="55dp"
            android:layout_height="35dp"
            android:layout_alignParentBottom="true"
            android:background="#767677" />

        <ImageView
            android:id="@+id/view_table8"
            android:layout_width="55dp"
            android:layout_height="35dp"
            android:layout_alignParentBottom="true"
            android:layout_toRightOf="@+id/view_table9"
            android:background="#767677" />

        <ImageView
            android:id="@+id/view_table5"
            android:layout_width="55dp"
            android:layout_height="35dp"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@+id/view_div4"
            android:background="#767677" />

        <ImageView
            android:id="@+id/view_table4"
            android:layout_width="55dp"
            android:layout_height="35dp"
            android:layout_alignParentBottom="true"
            android:layout_toRightOf="@+id/view_table5"
            android:background="#767677" />

        <ImageView
            android:id="@+id/view_table11"
            android:layout_width="50dp"
            android:layout_height="35dp"
            android:layout_above="@+id/view_table9"
            android:layout_marginBottom="15dp"
            android:background="#767677" />

        <ImageView
            android:id="@+id/view_table10"
            android:layout_width="50dp"
            android:layout_height="35dp"
            android:layout_above="@+id/view_table8"
            android:layout_marginBottom="15dp"
