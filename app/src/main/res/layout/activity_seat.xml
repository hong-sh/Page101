            android:background="#767677" />

        <ImageView
            android:id="@+id/view_table19"
            android:layout_width="50dp"
            android:layout_height="35dp"
            android:layout_below="@+id/view_div1"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@+id/view_table20"
            android:background="#767677" />

        <ImageView
            android:id="@+id/view_table17"
            android:layout_width="50dp"
            android:layout_height="35dp"
            android:layout_below="@+id/view_table20"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="20dp"
            android:background="#767677" />

        <ImageView
            android:id="@+id/view_table16"
            android:layout_width="50dp"
            android:layout_height="35dp"
            android:layout_below="@+id/view_table20"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:layout_toRightOf="@+id/view_table17"
            android:background="#767677" />

        <ImageView
            android:id="@+id/view_table14"
            android:layout_width="50dp"
            android:layout_height="35dp"
            android:layout_below="@+id/view_table17"
            android:layout_marginLeft="10dp"
